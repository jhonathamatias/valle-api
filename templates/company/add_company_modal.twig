<!-- Modal Structure -->
<div id="add-company" class="modal">
	<div class="modal-content">
		<h5>Adicionar empresa</h5>
		<form id="company-form" autocomplete="off">
			<div class="row">
				<div class="input-field col s12">
					<input id="company_name" autocomplete="off" class="form-field" data-name="Nome" name="company_name" type="text" class="validate">
					<label for="company_name">Nome</label>
					<span class="helper-text" data-error="wrong" data-success=""></span>
				</div>

				<div class="input-field col s12">
					<input id="company_email" class="form-field" data-name="Email" name="company_email" type="text" class="validate">
					<label for="company_email">Email</label>
					<span class="helper-text" data-error="wrong" data-success=""></span>
				</div>
				<div class="input-field col s12">
					<input id="company_phone" class="form-field" data-name="Telefone" name="company_phone" type="text" class="validate">
					<label for="company_phone">Telefone</label>
					<span class="helper-text" data-error="wrong" data-success=""></span>
				</div>
				<div class="input-field col s12">
					<input id="company_document" class="form-field" data-name="Telefone" name="company_document" type="text" class="validate">
					<label for="company_document">Documento</label>
					<span class="helper-text" data-error="wrong" data-success=""></span>
				</div>
			</div>
		</form>
		<div class="modal-footer">
			<div class="col s12">
				<button class="btn-flat modal-close waves-effect text-red" onclick="formReset()">
					<i class="material-icons left">close</i>
					<strong>Cancelar</strong>
				</button>
				<button class="btn-flat waves-effect" id="company-create" name="action">
					<i class="material-icons left">save</i>
					<strong>Criar</strong>
				</button>
			</div>
		</div>
	</div>
